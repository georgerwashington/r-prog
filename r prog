import java.io.*;
import java.util.*;

public class TestClass {
	 public static void main(String[] args) { 
		Scanner cin = new Scanner(System.in);
       int num = cin.nextInt();
       String s = String.format("First %d prime numbers are :-",num);
       System.out.println(s);
       int i = 2;
       int prime = 1;
       while (num!=0){
       prime = chk_prime(i);
       if (prime == 0){
       System.out.println(i);
       num--;}
       i++;}
	}
  public static int chk_prime(int i){
  int x,count;
   count = 0;
    x = i-1;
    while(x>1){
    if (i%x == 0)
    count++;
      x--;
    }
 if (count!=0)
  return count;
  return 0;}
}



import java.io.*;
import java.util.*;
import java.lang.*;

public class TestClass {
	 public static void main(String[] args) { 
		Scanner S = new Scanner(System.in);
       float P = S.nextFloat();
       Float ir = S.nextFloat();
       float Tax = S.nextFloat();
       float D = S.nextFloat();
       if (P == D)
         System.out.println("0");
       else{
       D /= P;
       Tax = (1+ir+(ir*Tax));
       D = Math.log(D);
       Tax = Math.log(Tax);
       float yy = D/Tax;
       String x = String.format("%.0f",yy);
       System.out.println(x);}
	}
}


#include <iostream>
#include <stdlib.h>
using namespace std;

int compttr(const void* a,const void* b){
return (*(int*)a - *(int*)b);}

int main()
{int n,*ptr;
 cin>>n;
 ptr = (int*)calloc(n,sizeof(int));
 int i = 0;
 int temp;
 for (i=0; i<n; i++)
  cin>>ptr[i];
 qsort(ptr,n,sizeof(int),compttr);
 cout<<"Minimum element in an array : "<<ptr[0];
 cout<<endl<<"Maximum element in an array : "<<ptr[n-1];
	return 0;
}

PROGRAM:
DATA SEGMENT
    N1 DW 0004H 
    N2 DW 0002H
    RES DW ?
    DATA ENDS
CODE SEGMENT
    ASSUME CS:CODE, DS:DATA
    START:MOV AX,DATA
    MOV DS,AX
    MOV AX,N1
    MOV BX,N2
    SUB AX,BX
    INT 21H
    CODE ENDS
END START
    


